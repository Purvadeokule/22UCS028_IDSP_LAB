flowers = ["Rose", "Tulips", "Jasmin"]

# Accessing first element in array
flower = flowers[0]
print(flower)

# Using insert function
flowers.insert(1, "Orchid")
print(flowers)

# Using append()
flowers.append("Lily")
print(flowers)

# Slicing methods
# Printing whole array
print(flowers[:])

# Print from selected index to end
print(flowers[2:])

# Print selected range
print(flowers[1:3])

# Printing in reverse direction
print(flowers[::-1])
